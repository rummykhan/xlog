{
    "name": "rummykhan/xlog",
    "description": "A PHP Laravel package to log all the requests coming to your website",
    "type": "library",
    "require": {
        "laravel/framework": "5.4.*",
        "torann/geoip": "1.0.*",
        "jenssegers/agent": "^2.5",
        "jenssegers/mongodb": "^3.2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Rehan Manzoor",
            "email": "rehan_manzoor@outlook.com"
        }
    ],
    "autoload":{
        "psr-4": {
            "RummyKhan\\XLog\\" : "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
              "RummyKhan\\XLog\\XLogServiceProvider"
            ]
        }
    }
}
